  <body>
    
    
        <div class="container" id="headContainer">
          <div class="row">
            <div class="col-2">
              <nav class="nav flex-column">
                <a class="nav-link active text-white" href="/login">Login</a>
                <a class="nav-link active text-white" href="/signup">Sign Up</a>
                <a class="nav-link active text-white" href="/logout">Logout</a>
              </nav>
            </div>

            <div class="col-10" id="heading">
              <h1 class="display-3">National Parks Observer</h1>
              <p class="lead">Site Dedicated to Indian National Parks</p>
             </div>

            
         </div>
        </div>

        <div class="container" id="postings">
          <div class="row">
            <div class="col-4">
              <div class="card">
                <img class="card-img-top" src="https://upload.wikimedia.org/wikipedia/commons/7/75/Corbett_Scape.jpg" alt="Card image cap">
                <div class="card-body">
                  <h5 class="card-title">Jim Corbett</h5>
                  <a href="/NationalPark" class="btn btn-primary btn-sm">View Page</a>
                </div>
              </div>
           </div>
           <div class="col-4">
            <div class="card">
              <img class="card-img-top" src="https://upload.wikimedia.org/wikipedia/commons/f/f5/View_of_ranthambhore.jpg" alt="Card image cap">
              <div class="card-body">
                <h5 class="card-title">Ranthambhore</h5>
                
                <a href="/NationalPark" class="btn btn-primary btn-sm">View Page</a>
              </div>
            </div>
          </div>
          <div class="col-4">
            <div class="card">
              <img class="card-img-top" src="https://upload.wikimedia.org/wikipedia/commons/7/7d/Bandhavgarh_National_Park_Morning_Light.jpg" alt="Card image cap">
              <div class="card-body">
                <h5 class="card-title">Bandhavgarh</h5>
                
                <a href="/NationalPark" class="btn btn-primary btn-sm">View Page</a>
              </div>
            </div>
            </div>
        </div>
